@article{rosenbergerHabitsComputerUse2009,
  title = {The Habits of Computer Use},
  author = {Rosenberger, Robert},
  year = {2009},
  journal = {International Journal of Computing \& Information Technology},
  volume = {1},
  number = {1},
  pages = {22--28},
  abstract = {One way that issues of human-computer interaction (HCI) can be explored is through the philosophical analysis of the experience of computer use. I review and expand on insights from the phenomenological tradition of philosophy, and apply them to the study of computer interface. In particular, I consider Don Ihde's philosophy of technology. His work examines the various ways that humans use technologies to act on and experience the world. This perspective is useful for drawing out the habits of experience that enable a computer user to approach such a complex and ever-evolving machine in a causal and everyday manner.},
  keywords = {cognition science,computer interface,hci,human-computer interaction,ihde,innovation,perception,phenomenology,post,postphenomenology,printed,sedimentation,trasnparency}
}

