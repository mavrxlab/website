@article{rosenbergerVariationalCrossexaminationMethod2020,
  title = {On Variational Cross-Examination: A Method for Postphenomenological Multistability},
  shorttitle = {On Variational Cross-Examination},
  author = {Rosenberger, Robert},
  year = {2020},
  month = aug,
  journal = {AI \& SOCIETY},
  issn = {1435-5655},
  doi = {10.1007/s00146-020-01050-7},
  urldate = {2022-03-26},
  abstract = {How should we understand postphenomenological methodology? Postphenomenology is a research perspective which builds on phenomenological and pragmatist philosophy to explore human\textendash technology relations, but one with open methodological questions. Here, I offer some thoughts on the epistemological processes that should be (and often implicitly may be) at work in this research. In particular, I am concerned with postphenomenological research on technological ``multistability,'' i.e., a device's ever-present capacity to be used for a variety of purposes, and to always be meaningful in multiple ways. I develop a methodology called ``variational cross-examination,'' which entails the critical contrast of a device's various stabilities. As a set of instructive examples, I draw on my own line of research on the politics of public spaces, and especially the critique of anti-homeless design.},
  langid = {english}
}

